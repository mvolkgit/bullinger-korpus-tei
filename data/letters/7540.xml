					<lb xml:id="p2z4"/>sub
					<lb xml:id="p2z5"/>1546.
					<lb xml:id="p2z6"/>ex
					<lb xml:id="p2z7"/>nihil boni
					<lb xml:id="p2z8"/>controversia, ex qua
					<lb xml:id="p2z9"/>sper
					<lb xml:id="p2z10"/>futurque
					<lb xml:id="p2z11"/>occogio
					<lb xml:id="p2z12"/>modi artibus, qui
					<lb xml:id="p2z13"/>popest. Multa susputant,
					<lb xml:id="p2z14"/>belli etc.
					<lb xml:id="p2z15"/>Tuus
					<lb xml:id="p2z16"/>1536.
					<lb xml:id="p2z17"/>et de
					<lb xml:id="p2z18"/>herren
					<lb xml:id="p2z19"/>integros esse sciam et
					<lb xml:id="p2z20"/>d.
					<lb xml:id="p2z21"/>S.
					<lb xml:id="p2z22"/>nihil dubitem
					<lb xml:id="p2z23"/>consciis.
					<lb xml:id="p2z24"/>hoc praestare possent,
					<lb xml:id="p2z25"/>vereor tuam
					<lb xml:id="p2z26"/>amplissimo
					<lb xml:id="p2z27"/>Civitatis eorum
					<lb xml:id="p2z28"/>et fideli gratiam
					<lb xml:id="p2z29"/>sint, quod me sibi fuerte boni viri plurimi
					<lb xml:id="p2z30"/>plus de illis pollicites.
					<lb xml:id="p2z31"/>moneo, ne sibi forte tam
					<lb xml:id="p2z32"/>vincetur, astque ita
					<lb xml:id="p2z33"/>ex per
					<lb xml:id="p2z34"/>quam postea sibi praestari
					<lb xml:id="p2z35"/>pernitiosissimo. Quicquid vero
					<lb xml:id="p2z36"/>S.
					<lb xml:id="p2z37"/>volvantur discribuantur
					<lb xml:id="p2z38"/>praestare possum,
					<lb xml:id="p2z39"/>intellexero ab illis et quicquid
					<lb xml:id="p2z40"/>d.
					<lb xml:id="p2z41"/>ex animo
					<lb xml:id="p2z42"/>toto pectore et lubentissimum
					<lb xml:id="p2z43"/>S.
					<lb xml:id="p2z44"/>ipse dirigat totum hoc negotium
					<lb xml:id="p2z45"/>obsecrans
					<lb xml:id="p2z46"/>24. iulii 1568.
					<lb xml:id="p2z47"/>testinantur Tigurinos,
					<lb xml:id="p2z48"/>S.
					<lb xml:id="p2z49"/>defero,
					<lb xml:id="p2z50"/>illustrissimo
					<lb xml:id="p2z51"/>omnia
					<lb xml:id="p2z52"/>S.
					<lb xml:id="p2z53"/>salo.
					<lb xml:id="p2z54"/>sedulo oro, ut servet
					<lb xml:id="p2z55"/>de
